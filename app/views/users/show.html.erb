<div class="contents row" >
  <p><%= @nickname%>さんの投稿一覧</p>
  <% @comments.each do |comment| %>
    <div class="content_post" style="background-image: url(<%= comment.image%>);">
      <%= comment.text%>
      <span class="name"><%= comment.name%></span>
    </div>
  <% end %>
  <%= paginate(@comments) %>
</div>